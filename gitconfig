[user]
	name = XXX_YOUR_NAME_XXX
	email = XXX@example.com
[core]
	excludesfile = /home/XXX/.gitignore
[alias]
    mybranch = rev-parse --abbrev-ref HEAD
    myb = rev-parse --abbrev-ref HEAD
    fp = "!sh -c 'git fetch XXX_remote1 \"$0\" && git pull XXX_remote1 \"$0\"'"
    fpg = "!sh -c 'git fetch XXX_remote2 `git mybranch` && git pull XXX_remote2 `git mybranch`'"
    co = checkout
    cob = checkout -b
    pushit = "!sh -c 'git push XXX_remote1 `git mybranch`'"
    pushitg = "!sh -c 'git push XXX_remote2 `git mybranch`'"

